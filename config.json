{
  "general":{
    "workingDir":"/root/api/",
    "domainFrontend":"localhost:4200",
    "protocolFrontend":"http"
  },
  "db":{
    "connectionString":"mongodb://localhost/deployhandlerapi"
  },
  "security":{
    "jwtSecret":"supersecret",
    "headerAllowOrigin":"*",
    "loginTokenExpire": "3600",
    "pwResetTokenExpire": "300"
  },
  "network":{
    "port": "3000",
    "securePort":"3001",
    "sslKey":"/etc/letsencrypt/live/api.deployhandler.com/privkey.pem",
    "sslCert":"/etc/letsencrypt/live/api.deployhandler.com/cert.pem"
  },
  "docs":{
    "docsPathJson":"./docs.json"
  }
}